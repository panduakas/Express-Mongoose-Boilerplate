{
  "name": "express-mongoose-boilerplate",
  "version": "1.0.0",
  "private": false,
  "description": "Nodejs using express mongoose",
  "main": "src",
  "engines": {
    "node": ">= 8.10.0",
    "yarn": ">= 1.15.2"
  },
  "_moduleAliases": {
    "@controllers": "src/controllers",
    "@helpers": "src/helpers",
    "@services": "src/services",
    "@models": "src/models"
  },
  "scripts": {
    "eslint": "node_modules/.bin/eslint --fix src/. --config .eslintrc.json",
    "start": "./node_modules/.bin/link-module-alias && node_modules/.bin/nodemon --exec node src/",
    "test": "node_modules/.bin/nodemon --exec node src/"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:panduakas/Express-Mongoose-Boilerplate.git"
  },
  "author": "Pandu Akas",
  "license": "ISC",
  "dependencies": {
    "chalk": "^2.4.2",
    "compression": "^1.7.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.9",
    "dotenv": "^6.0.0",
    "eslint": "^5.4.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.2",
    "express": "^4.16.3",
    "express-validator": "^5.3.1",
    "helmet": "^3.13.0",
    "http-errors": "~1.6.2",
    "link-module-alias": "^1.2.0",
    "lint-staged": "^7.2.2",
    "module-alias": "^2.1.0",
    "mongoose": "^5.6.5",
    "morgan": "~1.9.0",
    "node-uuid": "^1.4.8",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "prettier-eslint": "^8.8.2",
    "pretty-response": "^1.0.1",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "pretty-quick": "^1.10.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
